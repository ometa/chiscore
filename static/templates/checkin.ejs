<li data-team-id="<%= checkin.team.id %>">
  <span class="smiley">&#x1F642;  </span>
  <span class="team-id"><%= checkin.team.id %></span>
  <span class="team-name"><%= checkin.team.name %></span>
  <span class="team-time">
    <%= ChiScore.util.displayableTime(checkin.time) %>
  </span>

  <% if(!ChiScore.isPublic || ChiScore.canEarlyCheckout(checkin.time)) { %>
    <div class="check-out-link">&#x2715;</div>
  <% } %>
</li>
